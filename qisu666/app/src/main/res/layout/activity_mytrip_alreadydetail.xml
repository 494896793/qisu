                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="right"
                            android:padding="10dp"
                            android:text="2018-02-23-12:22:22"
                            android:textColor="@color/text_white"
                            android:textSize="15sp" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:layout_marginTop="@dimen/dimen_2dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/bianxian" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="总计费用"

                        android:textColor="@color/text_white"
                        android:textSize="15sp" />

                    <TextView
                        android:id="@+id/mytrip_alreadydetail_totalmoney"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:padding="10dp"
                        android:text="2018-02-23-12:22:22"
                        android:textColor="@color/text_white"
                        android:textSize="15sp" />
                </LinearLayout>

                <View
                    android:layout_marginTop="@dimen/dimen_2dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/bianxian" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="实际费用"
                        android:textColor="@color/text_white"
                        android:textSize="15sp" />

                    <LinearLayout
                        android:layout_marginRight="10dp"
                        android:paddingTop="10dp"
                        android:paddingBottom="10dp"
                        android:gravity="right"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_marginRight="@dimen/dimen_5dp"
                            android:id="@+id/mytrip_alreadydetail_paymoney"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:text="2018-02-23-12:22:22"
                            android:textColor="@color/text_white"
                            android:textSize="15sp" />

                        <ImageView
                            android:id="@+id/mytripdetail_tips"
                            android:layout_gravity="center"
                            android:src="@mipmap/wenhao"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_marginTop="@dimen/dimen_2dp"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginRight="10dp"
                    android:background="@color/bianxian" />

                <TextView
                    android:id="@+id/mytripdetail_tips1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@mipmap/tishi"
                    android:drawablePadding="5dp"
                    android:gravity="center|left"
                    android:paddingTop="@dimen/dimen_20dp"
                    android:paddingBottom="@dimen/dimen_20dp"
                    android:text=" 提示：消费不足9元，按最低9元计算！"
                    android:textColor="@color/orange"
                    android:textSize="13sp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="80dp"/>

        </LinearLayout>
    </ScrollView>
