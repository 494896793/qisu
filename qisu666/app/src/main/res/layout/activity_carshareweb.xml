<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <include layout="@layout/layout_title_primary" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:background="@color/bg_primary"
        >

        <TextView
            android:id="@+id/carshareweb_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="奇速共享汽车使用协议"
            android:textColor="@color/text_black"
            android:layout_gravity="center_vertical"
            android:textStyle="bold"
            android:textSize="16dp"
            android:layout_marginLeft="15dp"
            />

    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        >
        <www.qisu666.sdk.partner.widget.WebViews
            android:id="@+id/carshareweb_webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"  />

        <ImageView
            android:id="@+id/carshareweb_top"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@mipmap/top"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            />

    </RelativeLayout>



    <LinearLayout
        android:id="@+id/carshareweb_bottom"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:background="@color/bg_primary"
        >

        <TextView
            android:id="@+id/carshareweb_no"
            android:layout_width="96dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:text="不同意"
            android:textColor="@color/text_black"
            android:textSize="16dp"
            android:textStyle="bold"
            android:gravity="center"
            android:background="?android:attr/selectableItemBackground"
            />

        <View
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>

        <TextView
            android:id="@+id/carshareweb_yes"
            android:layout_width="96dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:text="我同意"
            android:textColor="@color/text_blue"
            android:textSize="16dp"
            android:textStyle="bold"
            android:gravity="center"
            android:background="?android:attr/selectableItemBackground"
            />


    </LinearLayout>


</LinearLayout>
